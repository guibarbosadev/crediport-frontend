{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rulesDirectory": [
        "tsconfig.json"
    ],
    "rules": {
        "prettier": false,
        "interface-name": false,
        "semicolon": true,
        "trailing-comma": false,
        "arrow-parens": true,
        "eofline": true,
        "jsx-no-lambda": false
    }
}